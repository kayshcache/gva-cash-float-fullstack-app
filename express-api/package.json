{
  "name": "express-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run build && node app-compiled.js",
    "build": "babel-node ./bin/www --out-file app-compiled.js",
    "dev": "nodemon --exec babel-node ./bin/www",
    "webpack-start": "webpack-dev-server --output-public-path=/build/ --mode development --progress --open --hot",
    "webpack-build": "webpack --mode production --progress"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "helmet": "^3.21.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.3",
    "morgan": "~1.9.1",
    "redis": "^3.0.2",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "babel-loader": "^8.1.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
